{% extends 'base.html.twig' %}

{% block title %}{{ parent()}} - Research_other_profiles{% endblock %}

{% block body %}
<h2>{{profil.pseudo}}</h2>
    <ul>
        <li>{{profil.prenom}} </li>
        <li>{{profil.nom}} </li>
        <li>{{profil.telephone}} </li>
        <li>{{profil.email}} </li>
        <li>{{profil.campus.nom}} </li>
    </ul>

    <div class="col-md-2 offset-md-1">
        <a href="{{ path("home") }}">
            <input class="btn btn-success"
                   type="button"
                   value="Retour">
        </a>
    </div>
{% endblock %}



























